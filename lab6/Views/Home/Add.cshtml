@model Car
<style>
    * {
        margin: 0;
        padding: 0;
        box-sizing: border-box;
    }

    li {
        list-style-type: none;
        padding: 10px;
    }

    .flex {
        display: flex;
    }
</style>

<form asp-action="Add" asp-controller="Home">
    <div class="flex">
        <div>
            <ul>
                <li>
                    <label asp-for="Model">Enter Model:</label>
                </li>
                <li>
                    <label asp-for="Price">Enter Price:</label>
                </li>
            </ul>
        </div>
        <div>
            <ul>
                <li>
                    <input asp-for="Model" type="text" autocomplete="off" />
                </li>
                <li>
                    <input asp-for="Price" type="text" autocomplete="off" />
                </li>
            </ul>
        </div>
    </div>
    <div>
        <input id="submbtn" type="submit" value="Add" class="btn btn-outline-dark" />
    </div>
</form>